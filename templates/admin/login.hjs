            </a>

        </div>
        <div class="collapse navbar-collapse" id="myNavbar">
            <div class="col-md-5">
                <ul class="nav navbar-nav">

                </ul>
            </div>

            <ul class="nav navbar-nav navbar-right">
                <li></li>
            </ul>

        </div>
    </div>
</nav>
    <div class="container">
        <div class="row">

            <div class="col-md-4 col-md-offset-4">

                <div class="login-panel panel panel-default" style="background:transparent">
                 <div class="panel-heading" style="display:none;">
                 <h2 class="" style="text-align:center;">SIGN IN</h2>
                  </div>
                    <div class="panel-body">
                        <form role="form" action="" method="post"  id="signinform">
                        <input type="hidden" name="formsubmitted" value="yes">
                            <fieldset>
                            <div id="userinfo" style="text-align:center;font-size:12px;margin-bottom:15px;">{{{ returninfo }}}</div>
                                <div class="form-group">
                                 <label>Username</label>
                                    <input class="form-control" name="username" type="text" required autofocus>
                                </div>
                                <div class="form-group">
                                <label>Password</label>
                                    <input class="form-control" name="password" type="password" required>
                                </div>
                                <input type="hidden" name="action" value="login">
                                <button type="submit" class="btn btn-lg btn-block bluebg white">Login</button>
                            </fieldset>
                        </form>
                    </div>

                </div>
            </div>
        </div>
